{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"no-console": "warn",
		"no-dupe-args": "error",
		"no-dupe-keys": "error",
		"no-irregular-whitespace": "warn",
		"no-use-before-define": "off",
		"no-warning-comments": "warn",
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto",
				"singleQuote": true
			}
		],
		"semi": "warn"
	}
}
